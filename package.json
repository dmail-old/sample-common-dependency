{
	"name": "@dmail/sample-common-dependency",
	"version": "1.0.1",
	"licence": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "index.js",
	"devDependencies": {
		"@dmail/prettiest": "^0.1.3",
		"@dmail/shared-config": "^1.1.0",
		"@dmail/test": "^0.2.4",
		"babel-cli": "7.0.0-beta.2",
		"babel-core": "7.0.0-beta.2",
		"babel-eslint": "8.0.1",
		"babel-plugin-istanbul": "4.1.5",
		"babel-preset-env": "2.0.0-beta.2",
		"cross-env": "5.0.5",
		"eslint": "3.19.0",
		"eslint-plugin-import": "2.7.0",
		"nyc": "11.2.1",
		"prettier": "1.7.0",
		"rimraf": "2.6.2",
		"codecov": "2.3.0"
	},
	"scripts": {
		"compile": "babel . --out-dir dist --ignore=node_modules,dist --source-maps",
		"watch": "babel . --watch --out-dir dist --ignore=node_modules,dist --source-maps",
		"clean": "rimraf dist",
		"dist": "npm run clean && npm run compile",
		"prepublishOnly": "npm run dist",
		"eslint": "eslint .",
		"prettiest": "prettiest",
		"pretest": "npm run eslint && npm run prettiest && npm run compile",
		"test": "test-run",
		"test-list": "test-list",
		"test-with-coverage": "cross-env NODE_ENV=test && nyc npm test",
		"coverage-report": "nyc report --reporter=text-lcov > coverage.lcov",
		"coverage-upload": "codecov --token=823ba31e-8d6b-45d5-8a79-20dc330c720c",
		"coverage": "npm run coverage-report && npm run coverage-upload"
	}
}
